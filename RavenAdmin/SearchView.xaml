		<DataTemplate x:Key="DocumentTemplate">
			<Grid Margin="12,0,12,6">
				<Rectangle Fill="White" />
				<Rectangle HorizontalAlignment="Left"
						   Width="10"
						   Fill="{Binding type, Converter={StaticResource documentTypeToBrush}}" />
