								   d:LayoutOverrides="Width"
								   FontSize="10.667"
								   HorizontalAlignment="Left"
								   TextWrapping="Wrap"
								   FontFamily="Monaco"
								   Margin="12,0,0,0" />
					</StackPanel>
				</Grid>
			</Grid>
		</DataTemplate>

		<ItemsPanelTemplate x:Key="ItemsPanelTemplate1">
			<StackPanel Orientation="Horizontal" />
		</ItemsPanelTemplate>

